	$.confirm("设置主题为精华", function() {
		var tids = xn.implode('_', modtid);
		var digest = $('input[name="digest"]').checked();
		var skill_type = $('input[name="skill_type"]').checked();
		var postdata = {digest: digest, skill_type:skill_type};
